{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/gpack/bin/Debug/netcoreapp2.1/gpack.dll",
            "args": [
                "compress",
                "d:\\SSMS-Setup-RUS.exe",
                "d:\\SSMS-Setup-RUS.exe.pack"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}